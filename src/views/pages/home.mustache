{{>partials/header}}
<section>
            <h1>{{titlePage}}</h1>
            
            
            <div class="container-news">
            {{#news}}
                <a id="#" href="#">
                    <article class="news-article">
                        <div class="news-article-img">
                            <img src="{{urlToImage}}" alt="">
                        </div>
                        <div class="article-texts">
                            <h2 class="news-article-title">{{title}}</h2>
                        <div class="article-info">
                            <div class="article-date">{{publishedAt}}</div>
                            <div class="article-source">Source: {{source.name}}</div>
                        </div>
                        <p class="article-description">{{description}}</p>
                        </div>
                    </article>
                </a>
                {{/news}}
            </div>
            
</section>

{{>partials/footer}}